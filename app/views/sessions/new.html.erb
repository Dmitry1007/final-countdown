<h1>Log In</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %> <br>

  <%= f.label :password %>
  <%= f.password_field :password %> <br>

  <%= f.label          :password_confirmation %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Login" %>
<% end %>
